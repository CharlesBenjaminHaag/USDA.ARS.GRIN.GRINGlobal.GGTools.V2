@model USDA.ARS.GRIN.GGTools.ViewModelLayer.SysUserViewModel
<div class="card card-row card-primary">
    <div class="card-header">
        <h3 class="card-title">
            Assigned Groups
        </h3>
    </div>
    <div class="card-body">
        <button id="btnUnAssign" class="btn btn-default btn-block" type="button"><<</button>
        <br>
        <table id="data-table-assigned-sys-groups" class="table table-striped" style="width:100%;">
            <thead>
                <tr>
                    <th></th>
                    <th>ID</th>
                    <th>Name</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var result in Model.DataCollectionAssignedSysGroups)
                {
                <tr>
                    <td>
                        @result.ID
                    </td>
                    <td>
                        @result.ID
                    </td>
                    <td>
                        @result.GroupTag
                    </td>
                </tr>
                }
            </tbody>
        </table>
    </div>
</div>
<script type="text/javascript">
    $(document).ready(function () {
        InitDataTableLightMultiSelect("data-table-assigned-sys-groups");
    });
</script>
