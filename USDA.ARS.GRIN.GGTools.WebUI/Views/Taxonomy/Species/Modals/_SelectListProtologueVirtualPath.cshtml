@model USDA.ARS.GRIN.GGTools.Taxonomy.ViewModelLayer.SpeciesViewModel
<hr />
<button id="btnSaveProtologue1" type="button" class="btn btn-default float-right" data-dismiss="modal">Add Selected Protologue</button>
<table id="data-table-protologue-lookup" class="table table-striped ggtools" style="width:100%">
    <thead>
        <tr>
            <th></th>
            <th>Protologue</th>
        </tr>
    </thead>
    <tbody>
        @if (Model.DataCollectionProtologues.Count() > 0)
        {
            foreach (var result in Model.DataCollectionProtologues)
            {
                <tr>
                    <td>
                        @result.ID
                    </td>
                    <td>
                        @*@WebUtility.HtmlDecode(result.Value)*@
                        @Html.Raw(result.Value)
                    </td>
                </tr>
            }
        }
        else
        {
            <tr>
                <td colspan="3">
                    <div class="callout callout-info" style="vertical-align:middle; margin-top:5px;">
                        Your search returned no matches.
                    </div>
                </td>
            </tr>
        }
    </tbody>
</table>
<button id="btnSaveProtologue2" type="button" class="btn btn-default float-right" data-dismiss="modal">Add Selected Protologue</button>
<script type="text/javascript">
    $(document).ready(function () {
        InitDataTableLookupFormat("data-table-protologue-lookup");
    });
</script>