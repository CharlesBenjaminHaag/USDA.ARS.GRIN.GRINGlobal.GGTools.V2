@model USDA.ARS.GRIN.GGTools.Taxonomy.ViewModelLayer.GenusViewModel
<hr />
<button id="btnSaveGenus1" type="button" class="btn btn-default float-right" data-dismiss="modal">Add Selected Genus</button>
<table id="data-table-genus-lookup" class="table table-striped ggtools" style="width:100%">
    <thead>
        <tr>
            <th></th>
            <th></th>
            <th>ID</th>
            <th>Name</th>
            <th>Accepted</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var result in Model.DataCollection)
        {
        <tr>
            <td>@result.ID</td>
            <td>@result.Name</td>
            <td>@result.ID</td>
            <td>@Html.Raw(result.AssembledName)</td>
            <td>@Html.Raw(Model.FormatBoolean(result.IsAcceptedName))</td>
        </tr>
        }
    </tbody>
</table>
<button id="btnSaveGenus2" type="button" class="btn btn-default float-right" data-dismiss="modal">Add Selected Genus</button>
<script type="text/javascript">
    $(document).ready(function () {
        InitDataTableWithAssembledName("data-table-genus-lookup");
    });
</script>