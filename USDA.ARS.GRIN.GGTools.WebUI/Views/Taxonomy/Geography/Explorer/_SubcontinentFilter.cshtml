@model USDA.ARS.GRIN.GGTools.Taxonomy.ViewModelLayer.GeographyViewModel
<input type="hidden" id="hf_current_filter_web_order_request_status" />
<div class="card card-default">
    <div class="card-header">
        <h3 class="card-title">
            <i class="fas fa-filter"></i>
            Sub-Continents
        </h3>
    </div>
    <div class="card-body scroll-container-level2">
        <div id="section-geography-subcontinent" class="subcontinent">
            @foreach (var result in Model.DataCollectionSubContinents)
            {
               
                <label class="button-checkbox-level2">
                    <input id="filter-geography-subcontinent" 
                           type="checkbox"
                           class="subcontinent-selector"
                           data-ggtools-geography-id="@result.ID" />
                    <span>@result.SubContinent</span>
                </label>
            }
        </div>
    </div>
</div>
<script type="text/javascript">


</script>