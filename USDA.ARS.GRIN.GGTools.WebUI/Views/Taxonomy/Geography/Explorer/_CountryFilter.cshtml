@model USDA.ARS.GRIN.GGTools.Taxonomy.ViewModelLayer.GeographyViewModel
<input type="hidden" id="hf_current_filter_web_order_request_status" />
<div class="card card-default">
    <div class="card-header">
        <h3 class="card-title">
            <i class="fas fa-filter"></i>
            Countries
        </h3>
    </div>
    <div class="card-body scroll-container-level2">
        <div id="section-geography-country">
            @foreach (var result in Model.DataCollectionCountries)
            {
                <label class="button-checkbox-level2">
                    <input 
                           id="filter-geography-country" 
                           type="checkbox" 
                           class="country-selector"
                           data-ggtools-country-code="@result.CountryCode" />
                    <span>@result.CountryDescription</span>
                </label>
            }
        </div>
    </div>
</div>
<script type="text/javascript">


</script>