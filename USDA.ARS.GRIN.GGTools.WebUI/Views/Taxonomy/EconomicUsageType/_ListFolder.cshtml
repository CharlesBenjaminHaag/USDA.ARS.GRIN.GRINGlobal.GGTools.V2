@model USDA.ARS.GRIN.GGTools.Taxonomy.ViewModelLayer.EconomicUsageTypeViewModel
<table id="data_table_taxonomy_economic_usage_type" class="table table-striped ggtools-table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Item</th>
            <th>Key</th>
            <th>Usage Code</th>
            <th>Usage Type</th>
            <th>Note</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var result in Model.DataCollection)
        {
        <tr>
            <td>@result.ID</td>
            <td>    </td>
            <td>    </td>
            <td>
                @result.EconomicUsageCode
            </td>
            <td>@result.UsageType</td>
            <td>
                @result.Note
            </td>
            <td>
                <a href="@Url.Action("Edit", "EconomicUsageType", new { entityId = result.ID })" class="btn btn-default btn-sm float-right row-edit">
                    <i class="fa fa-edit"></i>
                </a>
            </td>
        </tr>
        }
    </tbody>
</table>
<script type="text/javascript">
    $(document).ready(function () {
        InitDataTableFolderFormat("data_table_taxonomy_economic_usage_type");
    });
</script>
