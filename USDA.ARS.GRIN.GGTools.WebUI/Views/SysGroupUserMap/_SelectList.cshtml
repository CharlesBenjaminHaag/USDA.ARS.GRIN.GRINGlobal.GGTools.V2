@model USDA.ARS.GRIN.GGTools.ViewModelLayer.SysGroupUserMapViewModel

<div class="card">
    <div class="card-header">
        <h3 class="card-title">Groups (@Model.DataCollection.Count)</h3>
        <div class="card-tools">
            <button type="button" class="btn btn-tool" data-card-widget="collapse">
                <i class="fas fa-minus"></i>
            </button>
        </div>
    </div>
    <div id="section-groups" class="card-body p-0">
        <ul class="nav nav-pills flex-column">
            @foreach (var result in Model.DataCollection)
            {
                <li id="@result.SysGroupID" class="nav-item active">
                    <a href="#" class="nav-link">
                        <i class="fas fa-users"></i>@result.SysGroupID  @result.Title

                    </a>
                </li>
            }
        </ul>
    </div>

</div>







