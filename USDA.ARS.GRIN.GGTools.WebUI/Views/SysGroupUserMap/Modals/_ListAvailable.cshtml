@model USDA.ARS.GRIN.GGTools.ViewModelLayer.SysGroupUserMapViewModel
<table id="data-table-available-sys-groups" class="table table-striped" style="width:100%;">
    <thead>
        <tr>
            <th></th>
            <th>ID</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var result in Model.DataCollectionAvailable)
        {
            <tr>
                <td>
                    @result.ID
                </td>
                <td>
                    @result.ID
                </td>
                <td>
                    @result.GroupTag
                </td>
            </tr>
        }
    </tbody>
</table>

<script type="text/javascript">
    $(document).ready(function () {
        InitDataTableLightMultiSelect("data-table-available-sys-groups");
    });
</script>